<head>
  <!--Meta data-->
  <meta charset="utf-8" />
  <meta http-equiv="X-UA-Compatible" content="IE=edge" />
  <meta
    name="description"
    content="The home website for the discord bot Artemis, this website contains your dashboard and a control panel for mods+."
  />
  <meta property="og:image" content="/static/images/artava.png" />
  <meta http-equiv="Content-Type" content="text/html; charset=windows-1252" />

  <!--Title-->
  <title><%= test.client.user.username %></title>

  <!--favicon-->
  <link rel="shortcut icon" href="/static/images/logo2.svg" type="image/x-icon" />
  <link rel="icon" href="/static/images/logo2.svg" type="image/x-icon" />

  <!--Style shit-->
  <link href="/static/modules/style.css" rel="stylesheet" type="text/css" />
</head>
<body>
  <!--title-->
  <div class="container2">
    <div class="glitch">
      Project Artemis
      <img src="/static/images/logo2.svg" height="50px" width="50px" />
    </div>
    <p class="subtitle">Discord Bot</p>
  </div>

  <!--pixels-->
  <div id="container">
    <canvas id="pixie"></canvas>
  </div>

  <!--Moving background-->
  <div class="wrap">
    <div class="bg"></div>
    <div class="bg2"></div>

    <!--Menu-->
    <div class="menubar">
      <!--Login/logout/bot-->
      <button
        class="buttonlogin"
        style="background-color: rgba(255, 217, 0, 0.05);"
        onclick="location.href='/logout'"
        type="button"
      >
        <img
          src="https://cdn.discordapp.com/avatars/<%- userInfo.id %>/<%- userInfo.avatar %>.png"
          height="10px"
          width="10px"
          style="border-radius: 20px;"
        />
        <%- userInfo.username %> Click here to logout
      </button>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
      <button
        class="buttonlogin"
        style="background-color: rgba(255, 0, 0, 0.05);"
        type="button"
      >
        <img
          src="/static/images/artava.png"
          height="10px"
          width="10px"
          style="border-radius: 20px;"
        />
        <%- test.client.users.cache.size.toLocaleString() %> users / 
        <%- test.client.guilds.cache.size.toLocaleString() %> guilds | 
        <% let t = test.db.prepare("SELECT number FROM usage;").all(); %> 
        <% let str = 0; for (let i in t) str += t[i].number; %> 
        <%- str.toLocaleString() %>
        commands used
      </button>
      <br /><br />

      <!--Link Menu-->
      <button
        class="buttonlink"
        style="background-color: rgba(0, 255, 55, 0.05);"
        onclick="window.open('https://github.com/UtopicUnicorns/artemis','_blank')"
        type="button"
      >
        Github<br />
      </button>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
      <button
        class="buttonlink"
        style="background-color: rgba(0, 255, 255, 0.05);"
        onclick="window.open('https://www.paypal.com/paypalme2/UtopicUnicorn','_blank')"
        type="button"
      >
        Paypal
      </button>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
      <button
        class="buttonlink"
        style="background-color: rgba(0, 4, 255, 0.05);"
        onclick="window.open('https://discord.gg/EVVtPpw','_blank')"
        type="button"
      >
        Discord
      </button>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
      <button
        class="buttonlink"
        style="background-color: rgba(255, 0, 234, 0.05);"
        onclick="window.open('https://discordapp.com/api/oauth2/authorize?client_id=440892659264126997&permissions=8&scope=bot','_blank')"
        type="button"
      >
        Add Bot
      </button>
      <br /><br />

      <!--Site menu-->
      <button
        class="buttonmenu"
        style="background-color: rgba(255, 0, 221, 0.05);"
        onclick="location.href='/'"
        type="button"
      >
        User<br />Control
      </button>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
      <button
        class="buttonmenu"
        style="background-color: rgba(255, 153, 0, 0.05);"
        onclick="location.href='/board'"
        type="button"
      >
        Leader<br />Board
      </button>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
      <button
        class="buttonmenu"
        style="background-color: rgba(208, 255, 0, 0.05);"
        onclick="location.href='/control'"
        type="button"
      >
        Server<br />Control
      </button>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
      <button
        class="buttonmenu"
        style="background-color: rgba(140, 0, 255, 0.05);"
        onclick="location.href='/control2'"
        type="button"
      >
        Level-up<br />Roles
      </button>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
      <button
        class="buttonmenu"
        style="background-color: rgba(140, 0, 255, 0.05);"
        onclick="location.href='/command'"
        type="button"
      >
        Commands<br />Index
      </button>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
    </div>

    <!-- Actual content-->
    <br>
    <table width="800" border="0" align="center">
      <tr>
        <th scope="col">
          <!--Box-->
          <div class="contentbox">
            <!--CUT top-->